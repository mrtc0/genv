secretProvider:
  aws:
    - id: example-account
      service: SecretsManager
      region: ap-northeast-1
      auth:
        profile: default

envs:
  APP_ENV:
    value: "development"
  API_KEY:
    secretRef:
      provider: example-account
      key: apikey
  DB_PASSWORD:
    secretRef:
      provider: example-account
      key: db-credentials
      property: ".password"
